;;; The following lines added by ql:add-to-init-file:
#-quicklisp
(let ((quicklisp-init (merge-pathnames "quicklisp/setup.lisp"
                                       (user-homedir-pathname))))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)))

;;; Modified from https://gitlab.com/rigetti/forest/qvm/-/blob/master/doc/lisp-setup.md
#+quicklisp
(dolist (dir '("/Users/genos/qvm" "/Users/genos/quilc")) (push dir ql:*local-project-directories*))
