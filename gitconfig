[user]
	name = genos
	email = graham@quantifind.com
[alias]
	ci = commit
	di = diff --color-words
	st = status
	co = checkout
	rom = rebase origin/master
	romi = rebase -i --autosquash origin/master
	new = !git init && git symbolic-ref HEAD refs/heads/main
[color]
	ui = auto
[core]
	editor = nvim
	excludesfile = /Users/graham/.gitignore_global
[push]
    default = matching
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
