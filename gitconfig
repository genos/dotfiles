[user]
	name = Graham Enos
	email = graham@strangeworks.com
[alias]
	ci = commit
	di = diff --color-words
	st = status
	co = checkout
	rom = rebase origin/main
	romi = rebase -i --autosquash origin/main
	new = !git init && git symbolic-ref HEAD refs/heads/main
[color]
	ui = auto
[core]
	editor = nvim
	excludesfile = /Users/graham/.gitignore_global
[push]
    default = matching
[init]
	defaultBranch = main
[pull]
	ff = only
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
